distdir: $(EXTRA_DIST)
	@for file in $^; do \
	   dir=$$(echo "/$$file" | sed -e 's,/[^/]*$$,,'); \
	   if test ! -d "$(distdir)$$dir"; then mkdir -p "$(distdir)$$dir"; fi; \
	   cp -p $$file "$(distdir)/$$file" || exit 1; \
	done

.PHONY: all check clean distclean distdir
